<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <filter>
    <filter-name>ParameterFilter</filter-name>
    <filter-class>SecurityParamFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ParameterFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <servlet>
    <servlet-name>StationServlet</servlet-name>
    <servlet-class>Stations</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DistanceServlet</servlet-name>
    <servlet-class>StationsDistance</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>StationServlet</servlet-name>
    <url-pattern>/stations</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DistanceServlet</servlet-name>
    <url-pattern>/stationsDistance</url-pattern>
  </servlet-mapping>
</web-app>
